programa
{
	funcao inicio () 
	{
		real nota

		escreva("Insira sua nota: ")
		leia(nota)

		se (nota >= 6)
		{
			escreva("Aluno aprovado")
		}
		senao
		{
			escreva("Aluno reprovado")
		}
	}
}

